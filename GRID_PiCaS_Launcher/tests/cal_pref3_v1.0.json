{
    "PicasApiVersion": 0.5,
    "Job": {
        "NCPU": 6,
        "parameter_step": 1,
        "queue": "mediummc",
        "variables": {
            "OBSID": "L229585",
            "PIPELINE_STEP": "pref3_cal_CI",
